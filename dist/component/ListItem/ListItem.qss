  color: black;
  opacity: 1; }

.msg-con {
  margin-top: 40rpx;
  text-align: start; }

.name {
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 400;
  line-height: 43rpx;
  color: #797979;
  opacity: 1; }

.accept-reject {
  font-size: 32rpx;
  font-family: PingFang SC;
  font-weight: 400;
  line-height: 43rpx;
  color: black;
  opacity: 1; }

.tit-con {
  width: 95%;
