.list-item {
  width: 658rpx;
  height: 219rpx;
  margin: 25rpx auto;
  padding: 30rpx 0 0 34rpx;
  background: white;
  border: 5rpx solid #ffe0b4;
  opacity: 1;
  text-align: start;
  border-radius: 30rpx; }

.time {
  width: 101rpx;
  height: 32rpx;
  font-size: 24rpx;
  font-family: PingFang SC;
  font-weight: 400;
  line-height: 32rpx;
  color: black;
  opacity: 1;
  float: right; }

.float-con {
  width: 95%; }

.tag {
  width: 123rpx;
  height: 46rpx;
