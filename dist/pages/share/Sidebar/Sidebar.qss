.shadow-container {
  position: absolute;
  top: 0;
  left: 0;
  z-index: 15;
  width: 100%;
  height: 100%;
  overflow: hidden;
  background-color: rgba(0, 0, 0, 0.5); }

.body-container {
  position: absolute;
  top: 0;
  width: 88%;
  height: 100%;
  border: none;
  margin: none;
  background-color: white; }

.out {
  right: 0;
  animation: flyout 0.4s;
  -webkit-animation: flyout 0.4s; }

.in {
  right: -88%;
  animation: flyin 0.3s;
  -webkit-animation: flyin 0.3s; }

@-webkit-keyframes flyout {
  from {
    right: -88%; }
  to {
    right: 0; } }

@keyframes flyout {
  from {
    right: -88%; }
  to {
    right: 0; } }

@-webkit-keyframes flyin {
  from {
    right: 0; }
  to {
    right: -88%; } }

@keyframes flyin {
  from {
    right: 0; }
  to {
    right: -88%; } }

.choose-container {
